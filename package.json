{
	"name": "przytulanka",
	"version": "0.1.0",
	"license": "MIT",
	"author": "lasmedia <lasmedia2018@gmail.com>",
	"scripts": {
		"start": "gatsby develop",
		"build": "gatsby build",
		"clean": "gatsby clean",
		"format": "prettier --write \"**/*.{js,jsx,json,md}\"",
		"eslint": "eslint \"**/*.{js,jsx}\" --fix",
		"stylelint": "stylelint \"**/*.{js,css,scss}\" --fix",
		"prelint": "npm run format",
		"lint": "npm run stylelint",
		"postlint": "npm run eslint",
		"proxy": "npx netlify-cms-proxy-server"
	},
	"dependencies": {
		"babel-plugin-styled-components": "^2.1.4",
		"gatsby": "^5.14.5",
		"gatsby-image": "^3.11.0",
		"gatsby-plugin-manifest": "^5.14.0",
		"gatsby-plugin-netlify-cms": "^7.12.1",
		"gatsby-plugin-react-helmet": "^6.14.0",
		"gatsby-plugin-remove-serviceworker": "^1.0.0",
		"gatsby-plugin-sharp": "^5.14.0",
		"gatsby-plugin-styled-components": "^6.14.0",
		"gatsby-source-filesystem": "^5.14.0",
		"gatsby-transformer-remark": "^6.14.0",
		"gatsby-transformer-sharp": "^5.14.0",
		"leaflet": "^1.7.1",
		"netlify-cms-app": "^2.12.20",
		"netlify-cms-locales": "^1.17.2",
		"prop-types": "^15.7.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-helmet": "^6.1.0",
		"react-leaflet": "^4.0.0",
		"react-player": "^2.16.0",
		"react-slick": "^0.30.3",
		"slick-carousel": "^1.8.1",
		"styled-components": "^6.1.19"
	},
	"devDependencies": {
		"babel-eslint": "^10.1.0",
		"eslint": "^7.8.1",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-react": "^7.20.6",
		"eslint-plugin-react-hooks": "^4.1.0",
		"gatsby-plugin-root-import": "^2.0.5",
		"husky": "^4.3.0",
		"lint-staged": "^10.3.0",
		"prettier": "^2.1.1",
		"stylelint": "^13.7.0",
		"stylelint-config-rational-order": "^0.1.2",
		"stylelint-config-standard": "^20.0.0",
		"stylelint-order": "^4.1.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"prettier --config .prettierrc --write",
			"stylelint --fix",
			"eslint --fix"
		],
		"*.{json,md}": [
			"prettier --config .prettierrc --write"
		],
		"*.{css,scss}": [
			"prettier --config .prettierrc --write",
			"stylelint --fix"
		]
	}
}
